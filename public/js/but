//Hays Room 206 button block 1/3
socket.on('h1 1/3',  function(){
yesBtn([starth1_but]);
noBtn([prh1_but, stoph1_but]);
tith1.innerHTML = 'Patient waiting in Room 206';
tith1.classList.toggle("h4");
});
//Hays Room 206 button block 2/3
socket.on('h1 2/3', function(){
yesBtn([stoph1_but]);
noBtn([starth1_but,prh1_but]);
tith1.innerHTML = 'Room 206 is currently in session';
tith1.classList.remove("h4");
});
//Hays Room 206 button block 3/3
socket.on('h1 3/3', function(){
yesBtn([prh1_but]);
noBtn([stoph1_but,starth1_but]);
tith1.innerHTML = 'Room 206 is no longer in session';
tith1.classList.toggle("h4");
});
//Hays Room 219 button block 1/3
